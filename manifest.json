{
  "name": "UnitedQuickUtility",
  "description": "A simple extension to improve productivitty.",
  "version": "1.1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "host_permissions": ["https://app.delvepoint.com/*"],
  "action": {
    "default_title": "Open Notepad"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://app.delvepoint.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
